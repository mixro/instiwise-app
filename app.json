{
  "expo": {
    "name": "INSTiWISE",
    "slug": "INSTiWISE",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "instiwise",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.micep.instiwise"
    },
    "android": {
      "package": "com.micep.instiwise",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/instiwise-icon-foreground.png",
        "backgroundImage": "./assets/images/instiwise-icon-background.png",
        "monochromeImage": "./assets/images/instiwise-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "android": { 
            "icon": "./assets/images/notification-icon.png" 
          },
          "icon": "./assets/images/icon.png",
          "color": "#0F8783"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.GET_THE_IOS_URL_SCHEMA"
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bbbc053b-8d7d-41a6-ba6c-a6a7bd716f29"
      }
    }
  }
}
